print("{} asd", args...)


Fmt::format2("{} {}", ..args): String{
 let f = Fmt::new();
 for arg in args{
  Debug::debug(arg, &f)
 }
 return f.buf;
}


let s = format("pre{}mid{}end", ...)
{
let f = Fmt::new();
f.print("pre");
a1.debug(f);
f.print("mid");
a2.debug(f);
f.print("end");
let s = f.unwrap();
}