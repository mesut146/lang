trait iter<T>{
  bool has();
  
  T next();
}

func join<T>(arr:iter<T> , f: func<string,T> , del:string): string{
    let sb = StringBuilder();
    while (it.has()) {
        sb.append(f(arr.next()));
        if (it.has()) {
            sb.append(" ");
        }
    }
    return sb.toString();
}
let f = (ch) => ch.binaryStr();
let s = join([1,2,3,4], (ch) => ch.binaryStr() , ",");
