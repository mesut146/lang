import * from "util"
import * as util from "util"
import format as fmt from "util"

class Node<T>{
  next?: Node<T>;
  value: int = 0;
}

func makeNode(arr: int[]): Node<int>{
  var pos: int = 0;
  var res: Node<int> = Node{value: arr[pos++]};
  while(pos < arr.size){
    res = Node{value: arr[pos++], next: res};
  }
  return res;
}

enum Color{
  WHITE,
  BLACK
}

interface a{
  func calc(): int;
}


func calc(a:int,b:int,carry?:int) : int
{
  if(carry){
    return a + b + carry;
  }
  else{
    return a + b;
  }
}

func def(a:int = 0);

calc(1,2);
calc(1,2,3);
def();
def(5);
