let add = (a: int, b: int) => a + b;
let div = (a: int, b: int) => {
    if(b == 0){
        throw "division by zero";
    }
    return a / b;
};

let s = add(5, 6);

func apply(f: func<int>(int, long), x: int, y: int){
  return f(x, y);
}

apply((a, b) => a - b, 7, 3);

